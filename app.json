{
  "expo": {
    "name": "video-learn-app",
    "slug": "video-learn-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",    
    "splash": {
      "image": "./assets/splash-icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": ["videolearnapp", "exp+video-learn-app"],
    "userInterfaceStyle": "automatic",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chamminaung.videolearnapp"
    },

    "android": {
      "package": "com.chamminaung.videolearnapp",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/android-icon-foreground.png"
      },
      "icon": "./assets/icon.png",
      "edgeToEdgeEnabled": true
    },

    "web": {
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ]
  }
}
